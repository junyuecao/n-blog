<%- include header %>
<ul class="breadcrumb">
  <li><a href="/">首页</a> <span class="divider">&gt;</span></li>
  <li class="active">登录</li>
</ul>
<%- include alert %>
<form method="post" class="form-horizontal" id="signin-form">
	<div class="control-group">
 		<label class="control-label" for="name">用户名：</label><input type="text" name="name" required/><br />
	</div>
  <div class="control-group">
  	<label class="control-label" for="name">密码： </label><input type="password" name="password" required/><br />
	</div>
	<div class="control-group">
    <div class="controls">
      <input type="submit" value="登录" class="btn btn-primary"/>
    </div>
  </div>
         
</form>
<script src="/public/libs/jquery-validator/jquery.validate.min.js"></script>
<script src="/public/libs/jquery-validator/messages_zh.js"></script>
<script>
  $('#signin-form').validate();
</script>
